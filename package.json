{
  "name": "@posix/px",
  "version": "0.0.1",
  "description": "Posix cross platform npm scripts.",
  "repository": "parro-it/px",
  "author": "Andrea Parodi <andrea@parro.it>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "ava && linterjs .",
    "dev": "ava --watch & linterjs . --watch",
    "start": "node -r @std/esm ./index.js"
  },
  "engines": {
    "node": ">8.8"
  },
  "keywords": [],
  "ava": {
    "require": [
      "@std/esm"
    ]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "devDependencies": {
    "@std/esm": "^0.18.0",
    "ava": "^0.24.0",
    "linterjs": "^1.11.0"
  }
}
