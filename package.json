{
  "name": "@posix/px",
  "version": "0.0.1",
  "description": "Posix cross platform npm scripts.",
  "repository": "parro-it/px",
  "author": "Andrea Parodi <andrea@parro.it>",
  "license": "MIT",
  "files": ["index.js", "src"],
  "scripts": {
    "test": "cross-env DEBUG=piper ava test/test-runtime.js",
    "dev": "ava --watch & linterjs . --watch",
    "start": "node -r @std/esm ./index.js"
  },
  "bin": {
    "px": "src/cli.js"
  },
  "engines": {
    "node": ">9"
  },
  "keywords": [],
  "ava": {
    "require": ["@std/esm"]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "execa": "^0.8.0",
    "linterjs": "^1.11.0"
  },
  "dependencies": {
    "@std/esm": "^0.18.0",
    "bash-ast-traverser": "^0.5.0",
    "bash-parser": "^0.5.0",
    "piper": "^0.4.1",
    "piper-process": "^1.2.0"
  }
}
